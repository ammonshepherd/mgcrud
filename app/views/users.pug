extends layout

block content
  if userInfo
    - var email = userInfo.email
    - var username = userInfo.username
  ul.collection
    li.collection-item.avatar
      i.material-icons.circle perm_identity
      span.title Username
      p= username
    li.collection-item.avatar
      i.material-icons.circle email
      span.title Email
      p= email
